<div class="row">
  <div>

    <ul>
    <% @accounts.each do |account| %>
      <li>
        <%= link_to account.name, edit_account_path(account) %>
        <%= maybe_favorite_account_tag(account) %>
      </li>
    <% end %>
    </ul>
  </div>

  <div>
     <%= link_to "Add", new_account_path, class: "button button-secondary" %>
  </div>
</div>
